# Epic Online Services Configuration
# Copy this file to res://.env and fill in your actual values
# DO NOT commit the .env file to version control!

# Supported formats:
# Standard:  KEY=value
# GDScript:  key: String = "value"

# Product Information
PRODUCT_NAME=MyGameName
PRODUCT_VERSION=1.0.0

# EOS Credentials (Get these from https://dev.epicgames.com/portal)
PRODUCT_ID=your_product_id_here
SANDBOX_ID=your_sandbox_id_here
DEPLOYMENT_ID=your_deployment_id_here
CLIENT_ID=your_client_id_here
CLIENT_SECRET=your_client_secret_here
ENCRYPTION_KEY=your_64_char_hex_encryption_key_here
